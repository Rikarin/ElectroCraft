{
  "schemaVersion": 1,
  "id": "electrocraft",
  "version": "${version}",
  "name": "ElectroCraft",
  "description": "Technic Mod with cables and stuff",
  "authors": [
    "Jiu"
  ],
  "contact": {
    "homepage": "https://example.com/",
    "issues": "https://example.com/",
    "sources": "https://example.com/"
  },
  "license": "MIT",
  "icon": "assets/electrocraft/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      {
        "adapter": "kotlin",
        "value": "com.rikarin.electrocraft.DataGeneration"
      }
    ],
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.rikarin.electrocraft.Main"
      }
    ]
  },
  "mixins": [
    "electrocraft.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabricloader}",
    "fabric-api": ">=${fabric_api}",
    "fabric-language-kotlin": ">=${fabric_language_kotlin}",
    "minecraft": ">=${minecraft}",
    "java": ">=${java}"
  }
}